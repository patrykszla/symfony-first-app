      <div class="article"> 
        <div class="div"><a href="{{ article.link }}"><h2>Article title: {{ article.title }}</h2></a></div>
        <div> 
          <span><b>Article content: </b><span>{{article.content}}</span> </span>
          
        </div> <br />
      </div>
    {% endfor %}
  </div>

{% endblock %}

